.container{
  display: flex;
  flex-direction: column;
  justify-content: start;
  align-items: center;
  padding: 0rpx 30rpx;
}

.logo{
  width: 144rpx;
  height: 144rpx;
  margin: 30rpx 0rpx;
}

.btn-login{
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
}

.wechat{
  margin-top: 30rpx;
  background-color: aqua;
  color: #fff;
}

.douban{
  background-color: #fff;
  color: aqua;
  border: 2rpx aqua solid;
  margin-top: 30rpx;
}

.btm-view{
  position: absolute;
  bottom: 20rpx;
  left: 0rpx;
  height: 100rpx;
  right: 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.btm-view .open {
  margin-left: 10rpx;
  color: aquamarine;
}